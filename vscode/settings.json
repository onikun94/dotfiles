{
	"workbench.colorTheme": "GitHub Dark Colorblind (Beta)",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorCustomizations": {
		"editor.background": "#222433",
		"editor.foreground": "#d4d4d4",
		"editorCursor.foreground": "#dcdcdc",
		"editor.lineHighlightBackground": "#333333",
		"editorLineNumber.foreground": "#858585",
		"activityBar.background": "#222433",
		"sideBar.background": "#222433",
		"statusBar.background": "#222433",
		"tab.inactiveBackground": "#222433",
		"editorGroupHeader.tabsBackground": "#222433"
	},
	"editor.codeActionsOnSave": {},
	"vim.useSystemClipboard": true,
	"editor.detectIndentation": false,
	"editor.tabSize": 2,
	"github.copilot.editor.enableAutoCompletions": true,
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"typescript.updateImportsOnFileMove.enabled": "always",
	"explorer.confirmDragAndDrop": false,
	"gitlens.launchpad.indicator.enabled": false,
	"yaml.format.singleQuote": true,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": true,
	"editor.accessibilitySupport": "off",
	"cursor.cpp.enablePartialAccepts": true,
	"extensions.experimental.affinity": {
		"asvetliakov.vscode-neovim": 1
	},
	"vim.handleKeys": {
		"<C-w>": true
	},
	"vscode-neovim.useCtrlKeysForNormalMode": true,
	"vscode-neovim.useCtrlKeysForInsertMode": true,
	"vim.useCtrlKeys": true,
	"json.schemas": []
}
